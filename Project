10th.php
<?php
include("connection.php");
$sql = "SELECT 10th FROM academicinfo  WHERE RegNo=3412108";
$result = $conn->query($sql);
if($result->num_rows > 0) 
{
    while($row= $result->fetch_assoc()) 
{
        echo "   ".$row["10th"]; 
}
 }else
{
    echo "0 results";
}
$conn->close();
?>

