Academicinfoinsert.php

<?php
include "connection.php";
$RegNo=$_POST["regno"];
$RollNo=$_POST["rollno"];
$Degree=$_POST["Degree"];
$course=$_POST["course"];
$Branch=$_POST["Branch"];
$Batch=$_POST["Batch"];
$Department=$_POST["Department"];
$Category=$_POST["Category"];
$section=$_POST["section"];
$tenth=$_POST["10th"];
$twelveth=$_POST["12th"];
$sql="INSERT INTO academicinfo (RegNo,RollNo,Degree,Course,Branch,Batch,Department,Category,Section,10th,12th) VALUES ('$RegNo','$RollNo','$Degree','$course','$Branch','$Batch','$Department','$Category','$section','$tenth','$twelveth')";

if($conn->query($sql)==TRUE)
{
include "contactinfo.php";
}
else
{echo "error";}

?>
Contactinfoinsert.php
<?php
include "connection.php";
$RegNo=$_POST["regno"];
$BP=$_POST["BP"];
$email=$_POST["email"];
$mobile=$_POST["mobile"];
$address=$_POST["address"];
$state=$_POST["state"];
$pincode=$_POST["pincode"];
$parentemail=$_POST["parentemail"];
$parentno=$_POST["parentno"];
$sql="INSERT INTO contactinfo (RegNo,BP,StudentEMail,MobNo,Address,State,Pincode,ParentEMail,ParentMobNo) VALUES ('$RegNo','$BP','$email','$mobile','$parentno','$address','$state','$pincode','$parentemail')";

if($conn->query($sql)==TRUE)
{
echo "THANK YOU FOR REGISTRATION";
}
else
{echo "error";}

?>

Personalinfoinsert.php
<?php
include "connection.php";
$RegNo=$_POST["regno"];
$Name=$_POST["Name"];
$Fathername=$_POST["fname"];
$DOB=$_POST["DOB"];
$Gender=$_POST["Gender"];
$BloadGroup=$_POST["BG"];
$Community=$_POST["community"];
$Nationality=$_POST["nationality"];
$sql="INSERT INTO personalinfo (RegNo,Name,Fathername,DOB,Gender,BloadGroup,Community,Nationality) VALUES ('$RegNo','$Name','$Fathername','$DOB','$Gender','$BloadGroup','$Community','$Nationality')";

if($conn->query($sql)==TRUE)
{
include "academicinfo.php";
}
else
{echo "error";}
